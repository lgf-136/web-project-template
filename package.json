    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
