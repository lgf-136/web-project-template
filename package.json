    "web",
    "template"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commit": "git-cz",
    "cz": "git-cz",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx,.vue test/ src/",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx,.vue test/ src/ --fix",
    "stylelint": "stylelint src/**/*.{vue,css,less,scss}",
    "stylelint:fix": "stylelint src/**/*.{vue,css,less,scss} --fix",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "postcss": "npx postcss src/index.css --dir dist/ ",
    "postcss:scss": "npx postcss src/index.scss --dir dist/ ",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
